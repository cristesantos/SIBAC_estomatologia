// Regras de evidências associadas a hipóteses

rule "Abscesso Pulpar - dor leve"
when
    $h : Hipotese(nome == "Abscesso Pulpar")
    Evidencia(tipo == "dor", valor == "leve")
then
    $h.atualizarFC(-0.5);
end

rule "Abscesso Pulpar - aparencia gengiva - lisa e brilhante"
when
    $h : Hipotese(nome == "Abscesso Pulpar")
